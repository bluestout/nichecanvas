<div class="Customsingle">
  {%- for block in section.blocks -%}
  <div>    
    <div class="cstSlideVOF">
      <div class="page-width">
        <div class="cstHingVo">
          <h2>{{block.settings.slide_title}}</h2>
          <p>{{block.settings.subheading}}</p>         
        </div>
        <div class="cstSinglBtn">
          <a href="{{block.settings.button_link}}">{{block.settings.button_label}}</a>
        </div>
      </div> 
      <img src="{{ block.settings.image | img_url: '1400x950' }}" alt="Image" title="Image">
    </div>
  </div>
  {%endfor%}
</div>

<div class="cstSinglBtnMob">
  <a href="{{block.settings.button_link}}">{{block.settings.button_label}}</a>
</div>

